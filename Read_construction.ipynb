    "# Transforms the final eulerian path into the final read\n",
    "\n",
    "def read_transformer(euler_path, distance):\n",
    "    read = euler_path[0][0]\n",
    "    read2 = euler_path[0][1] \n",
    "    c = 0\n",
    "    for node in euler_path:\n",
    "        if c > 0:\n",
    "            read += node[0][-1]\n",
    "            read2 += node[1][-1]\n",
    "        c+=1\n",
    "    print(read)\n",
    "    print(read2)\n",
    "    final_read = read + read2[-distance:]\n",
    "    \n",
    "    return final_read"
   ]
